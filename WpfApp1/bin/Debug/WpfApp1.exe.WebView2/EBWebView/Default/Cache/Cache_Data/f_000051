var onImgLoad = function(selector, callback){
    $(selector).each(function(){
        if (this.complete || /*for IE 10-*/ $(this).height() > 0) {
            callback.apply(this);
        }
        else {
            $(this).on('load', function(){
                callback.apply(this);
            });
        }
    });
};
//Предзагрузка изображений в логотипе
$('.logo__animation').imagesLoaded( { background: '.preload' }, function() {
  $('.logo__animation').css({'opacity':'1'});
});


//Проверка адреса email
function validateEmail(email) {
  var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
  return re.test(email);
}
// Функиция для заморозки изменения класса
function debounce(f, ms) {
	let state = null;
	const FROZEN = true;
	return function() {
		if(state) return;

		f.apply(this,arguments);
		state = FROZEN;
		setTimeout(function() {
			state = null
		}, ms);
	}
}
function heightMMtab() {
	const header = $('.header'),
	wrap = $('.wrap'),
	megamenu = $('.megamenu');

	var pT = $('.header').offset().top + $('.header').innerHeight();
	megamenu.css('paddingTop',pT + 'px');
	wrap.height($(window).height() - pT - 40);
}

function mmSecondTab() {
	if($(window).width() > 768) {
		$('.megamenu__slide-xs').css({'display':'block', 'left':'0'});
		let submenu = $('.megamenu__slide-xs').find('.submenu.active').attr('menu-data');
		$('.megamenu__main .lvl-1 a[menu-data = '+ submenu + ']').addClass('active');
	} else {
		$('.megamenu__main .lvl-1 a').removeClass('active');
		$('.megamenu__slide-xs').css({'display':'none', 'left':'100%'});
	}
}
// Фильтр для новостей
function SetDateSes(inM, inY, page)	{
	var d1 = "01." + inM + "." + inY;
	var countDay = new Date(inY, inM, 0).getDate();
	var d2 = countDay + "." + inM + "." + inY;
	if((inM == 0 && inY == 0) || (inY == 0))
	{
		d1 = "01.01.1990";
		d2 = "31.12.2099";
	}
	else if(inM == 0)
	{
		d1 = "01.01." + inY;
		d2 = "31.12." + inY;
	}
	if(page=='media') {
		$.ajax({
			type: "POST",
			url: "/local/templates/kemsu/ajax/setDateSes.php",
			data: {"DateFromM": d1, "DateToM": d2},
			success: function(mes){
				location.reload();
			}
		});
	} else {
		$.ajax({
			type: "POST",
			url: "/local/templates/kemsu/ajax/setDateSes.php",
			data: {"DateFrom": d1, "DateTo": d2},
			success: function(mes){
				location.reload();
			}
		});
	}
}
// Подсказки для календаря
function tooltip() {
	if(window.innerWidth > 991) {
		$("*[data-event]").mousemove(function (e) { 
			var tooltip = $(".tooltip[data-event="+ $(this).attr("data-event") + "]");
			var wBreak = window.innerWidth - $(this).offset().left - $(this).width();
			if(wBreak < 350) {
				tooltip.addClass('tooltip_bottom');
			} else {
				tooltip.removeClass('tooltip_bottom');
			}    
			tooltip
			.css({ 
				"top" : e.clientY,
				"left" : e.clientX
			})
			.show();
		}).mouseout(function () {
			var tooltip = $(".tooltip[data-event="+ $(this).attr("data-event") + "]");        
			tooltip
			.css({
				"top" : 0,
				"left" : 0
			}).hide();
		});
	}
}
function resize() {
	heightMMtab();
	mmSecondTab();
	tooltip();
	refreshVar();
}

var navPos, winPos;

function refreshVar() {
	navPos = $('.header').offset().top;
	if($(window).height() < $(document).height()) {
		if($('.wrapper-scroll-btn').hasClass('hide-scroll-btn')) {
			$('.wrapper-scroll-btn').removeClass('hide-scroll-btn');
		}
	} else {
		if(!$('.wrapper-scroll-btn').hasClass('hide-scroll-btn')) {
			$('.wrapper-scroll-btn').addClass('hide-scroll-btn');
		}
	}
}

function mosaic() {
    $('.photo').jMosaic('clear');
	$('.photo').jMosaic({
	  items_type: "div", 
	  min_row_height: 170,  
	  is_first_big: false,
	  margin:10
	});
	$('.photo img').css('opacity','1.0');
}

function shedule() {
	var institute = $.cookie('schedule_intsitute');
	var curs = $.cookie('schedule_curs');
	//console.log(curs)
	if(institute) {
		$(".select-tabs__tabs").removeClass("active");
		$(".select-tabs__tabs").hide();
		$(".select-tabs__tabs[data-select='"+institute+"']").show().addClass("active");
		$(".select-tabs__select option[data-select='"+institute+"']").attr("selected","selected");
		if(curs) {
			$(".select-tabs__tabs[data-select='"+institute+"']").find(".tabs-nav__item").removeClass("active");
			$(".select-tabs__tabs[data-select='"+institute+"']").find(".tabs-content").hide();
			$(".select-tabs__tabs[data-select='"+institute+"']").find(".tabs-content[data-for='"+curs+"']").show().addClass("active");
			$(".select-tabs__tabs[data-select='"+institute+"']").find(".tabs-nav__item[data-for='"+curs+"']").addClass("active");
		}
	}
}

function initTimer(timer) {
	timer.countdown(timer.attr("date-time"), function(e){
		var number = ['%D','%H','%M','%S'];
		for(var i = 0; i<number.length; i++ ) {
			timer.find("div:eq(" + i + ") span").text(e.strftime(number[i]));
		}
	});
}

window.onload = function() {
	mosaic();
}

$(document).ready(function() {
	$('.scrollbar-inner').scrollbar();
	
	resize();
	shedule();

	$('#phone').mask('+7 (999) 999-9999');

	$("[data-fancybox]").fancybox({
		lang: 'ru',
		share: false
	});

	$(window).scroll(function() {
		winPos = $(window).scrollTop();
		if (winPos > navPos) {
			$('.scrollup-btn').addClass('rotate');
		}else{
			$('.scrollup-btn').removeClass('rotate');
		}
	});
	$('.scrollup-btn').click(function(){
		if($(this).hasClass('rotate')){
			$('body,html').animate({scrollTop:0},800);
		}else{
			$('body,html').animate({scrollTop:$(document).height()},800);
		}
	});


	$('.cookie-block__close').click(function() {
		$('.cookie-block').slideUp(200);
		$.cookie('cookie_agree', true, { expires: 365 , path: '/'});
	});


	$('.has-sub').click(function (e) {
		var subMenu = $(this).next('.menu__tab');
		if(subMenu.hasClass('active')) {
			subMenu.stop().slideUp(300);
			subMenu.removeClass('active');
			if(subMenu.parent().hasClass('menu__item')) {
				$('.logo').removeClass('active');
			}
		} else {
			subMenu.removeClass('active');
			subMenu.stop().slideDown(300);
			subMenu.addClass('active');
			if(subMenu.parent().hasClass('menu__item')) {
				$('.logo').addClass('active');
			}
		}
		$(this).toggleClass('active');
		if($(this).parents('.wrap').length > 0) {
			heightMMtab();
		}
		return false;
	});

	$('.megamenu__main .lvl-1 a').click(function (e) {
		if($(window).width() < 768) {
			$('.megamenu__slide-xs').show(200);
			$('.megamenu__slide-xs').animate({left:'0'}, 500);
		}
		var submenu = $('.megamenu__center').find('.submenu[menu-data=' + $(this).attr('menu-data') + ']');
		if(!submenu.hasClass('active')) {
			$('.megamenu__main .lvl-1 a').removeClass('active');
			$('.megamenu__center .submenu').removeClass('active');
			$(this).addClass('active');
			submenu.addClass('active');
		}
		return false;
	});

	var flag = 0;
	$(".megamenu-opener").click(function() {
		if(!flag) {
			$("body").toggleClass("body-lock");
			$(".header").toggleClass("magamenu_active");
			$(".megamenu").toggleClass("active");
			$('.search').removeClass('active');
			$(".megamenu-opener").toggleClass("megamenu-opener__open");
			if ($(".megamenu-opener").hasClass('megamenu-opener__open')) {
				$('.search').removeClass('active');
				$('.logo').addClass('active');
			} else {
				$('.logo').removeClass('active');
			}
			if($(window).width() < 768 && $(".megamenu-opener").hasClass('megamenu-opener__open')) {
				$('.megamenu__main .lvl-1 a').removeClass('active');
			}
			flag = 1;
			setTimeout(function() {
				flag = 0;
			}, 1000);
		}
	});

	$(".megamenu .back").click(function() {
		$('.megamenu__slide-xs').animate({left:'100%'}, 500, function () {
			$('.megamenu__slide-xs').hide();
			$('.megamenu__main .lvl-1 a').removeClass('active');
		});

	});

	$(".tabs-nav__item").click(function() {
		var data_for = $(this).attr("data-for");
		var tabsBlock = $(this).parents( ".tabs-block" );
		if( !$(this).hasClass("active") ) {
			tabsBlock.find(".tabs-nav__item").removeClass("active");

			var tabsContents = [];
			tabsBlock.find( ".tabs-nav__item" ).each( (index,element) => {
				if ( data_for != $(element).attr( "data-for" ) ) {
					console.log( ".tabs-content[data-for='"+$(element).attr( "data-for" )+"']" );
					$(".tabs-content[data-for='"+$(element).attr( "data-for" )+"']").hide().removeClass( "active" );
				}
			});
			
			var tabsContentTarget = $(".tabs-content[data-for='"+data_for+"']");
			tabsContentTarget.show().addClass("active");
			$(this).addClass("active");
			
			if ( tabsContentTarget.attr( "data-for_group" ) ) {
				console.log( "activating for_group=" + tabsContentTarget.attr( "data-for_group" ) );
				console.log( tabsContentTarget.find( ".tabs-block" ).length );
				if ( tabsContentTarget.find( ".tabs-block" ).length ) {
					$( ".tabs-content[data-group]" ).not( "[data-group=" + tabsContentTarget.attr( "data-for_group" ) + "]" ).hide().removeClass( "active" );
					var activeSubItem = tabsContentTarget.find( ".tabs-block" ).find( ".tabs-nav__item.active" );
					activeSubItem.removeClass( "active" );
					activeSubItem.trigger( "click" );
				}
			}
			
			
			//$(".tabs-content").hide();
			//$(".tabs-content[data-for='"+data_for+"']").show().addClass("active");
		
		}

		if($(this).parents('div.select-tabs__tabs').is('.select-tabs__tabs')) {
			//console.log(data_for);
			$.cookie('schedule_curs', data_for);
		}
	});

	$(".select-tabs__select").change(function() {
		var data_select = $(this).find("option:selected").attr("data-select");
		var curs = $(".select-tabs__tabs[data-select='"+data_select+"']").find('.tabs-nav__item:first').attr('data-for');
		if(data_select) {
			$.cookie('schedule_intsitute', data_select, { expires: 300 , path: '/'});
			if($.cookie('schedule_curs')) {
				$.cookie('schedule_curs', curs);
			} else {
				$.cookie('schedule_curs', curs, { expires: 300 , path: '/'});
			}
		}
		if( !$(this).hasClass("active") ) {
			$(".select-tabs__tabs").removeClass("active");
			$(".select-tabs__tabs").hide();
			$(".select-tabs__tabs[data-select='"+data_select+"']").show().addClass("active");
		}
	});


	$('body').on({'mousewheel': function (e) {
		if($('body').hasClass('body-lock')){
			e.preventDefault();
			e.stopPropagation();
		}
	}});

	$('.category-list__item_main').click(function () {
		var category = $(this).parents('.category-list');
		category.toggleClass('active');
		return false;
	});

	initTimer($(".timer"));

	$('.slider').slick({
		autoplay: true,
		autoplaySpeed: 10000,
		arrows: false,
		dots: true,
		customPaging : function(slider, i) {
			return '<div class="page"></div>';
		}
	});

	$('.slider').on('beforeChange', function(event, slick, currentSlide, nextSlide){
		var $timer = $('.slider').find('*[data-slick-index=' + nextSlide + ']').find('.timer');
		if($timer.length) {
			initTimer($timer);
		}
	});


	$('.slider-about').slick({
		autoplay: true,
		dots:false,
		prevArrow: '<div class="prev"></div>',
		nextArrow: '<div class="next"></div>'
	});

	$('.direction-slider').slick({
		slidesToShow: 1,
		dots:false,
		slide: 'img',
		prevArrow: '<div class="prev"></div>',
		nextArrow: '<div class="next"></div>'
	});


	$('.direction-info-slider').slick({
		fade: true,
		cssEase: 'linear',
		draggable: false,
		touchMove: false,
		prevArrow: '<div class="prev"></div>',
		nextArrow: '<div class="next"></div>'
	});

	$('.direction-info-slider .prev').click(function() {
		$(".direction-info-slider").slick('slickPrev');;
	});

	$('.direction-info-slider .next').click(function() {
		$(".direction-info-slider").slick('slickNext');;
	});

	$('.ads-slider').slick({
		slidesToShow: 5,
		dots:false,
		arrows: false,
		slidesToScroll: 5
	});

	var dt;
	$('.events-carousel')
	.on('init', function(){
		$('.events-carousel').css('opacity','1');
		dt = $(this).find('.active').attr('dt');
		var it = $(this).find('.active'),
			next = $(this).find('.next'),
			prev = $(this).find('.prev');
		if(!it.next().hasClass("slick-slide")) {
			next.hide();
		} else {
			next.show();
		}
		if(!it.prev().hasClass("slick-slide")) {
			prev.hide();
		} else {
			prev.show();
		}
	})
	.slick({
	  slidesToShow: 3,
	  initialSlide: ($('.events-carousel .item').index($('.events-carousel').find('.active'))),
	  infinite: false,
	  centerMode: true,
	  focusOnSelect: true,
	  prevArrow: '<div class="prev"></div>',
	  nextArrow: '<div class="next"></div>',
		responsive: [
			{
				breakpoint: 768,
				settings: {
					centerMode: true,
					centerPadding: '40px',
					slidesToShow: 1
				}
			}
		]
	});

	$('.date-line')
	.on('init', function(){
		$('.date-line').css('opacity','1');
	})
	.slick({
		speed: 1000,
		initialSlide: ($('.date-line .item').index($('.date-line').find('*[dt=' + dt+ ']'))),
		slidesToShow: 15,
		centerMode: true,
		arrows: false,
		draggable: false,
		infinite: false,
		touchMove: false,
		responsive: [
			{
				breakpoint: 768,
				settings: {
					slidesToShow: 7
				}
			},
			{
				breakpoint: 400,
				settings: {
					slidesToShow: 3
				}
			}
		]
	 });

	$('.events-carousel').on('beforeChange', function(event, slick, currentSlide, nextSlide){
		var it =$('.events-carousel').find('*[data-slick-index=' + nextSlide + ']'),
			next = $(this).find('.next'),
			prev = $(this).find('.prev');
		if(!it.next().hasClass("slick-slide")) {
			next.hide();
		} else {
			next.show();
		}
		if(!it.prev().hasClass("slick-slide")) {
			prev.hide();
		} else {
			prev.show();
		}
		var showH = it.attr('dt');
		$('.date-line .item').removeClass('active');
		var aa = $('.date-line').find('*[dt=' + showH + ']');
		aa.addClass('active');
		$('.date-line').slick('slickGoTo', parseInt(aa.attr('data-slick-index')), true);
	});

	//Работа фильтра для календаря событий
	$('.events-filter select').change(function() {
		$('.events-filter #fMonth option[value="'+ $('#fMonth').val() +'"]').attr('selected', true);
		$('.events-filter #fYear option[value="'+ $('#fYear').val() +'"]').attr('selected', true);
		$('.events-filter form').submit();
	});
	//кнопка поиска
	$('.search__button').on('click',function() {
		const form = $(this).next('form');
		if(form.find('input').val() != "") {
			form.submit();
		} else {
			location.href = '/search/';
		}
	});

	// подбор направлений
	$('.directions-exams__item').click(function() {
		$(this).toggleClass('directions-exams__item_active');
		return false;
	});

	$('.directions-exams__button_send').click(function() {
		var query = "";
		$('.directions-exams__item_active').each(function() {
			if($(this).attr("data-exam") != "") {
				query += "&exams=" + $(this).attr("data-exam");
			}
		});
		location.href = '?' + query + "#directions-exams";
	});
  //Спойлер
  $('.spoiler__title').click(function() {
  	$(this).parent('.spoiler').toggleClass('spoiler_open');
  	$(this).next('.spoiler__content').slideToggle(400);
  });

	//Форма
	$('.check-form form').on('submit', function() {
		var errors = 0;
		const form = $(this);
		$(this).find('[required-form="required"]').each(function() {
			if($(this).val() == "") {
				errors++;
				$(this).addClass('error');
			} else $(this).removeClass('error');

			if($(this).is("#email")) {
				if(!validateEmail($(this).val())) {
					errors++;
					$(this).addClass('error');
				} else $(this).removeClass('error');
			}
		});
		$(this).find('.dropdown').each(function() {
			if($(this).find('.selected').text() == "-Выбрать-") {
				errors++;
				$(this).addClass('error');
			} else $(this).removeClass('error');

		});

		if(errors>0) {return false;}

		if($(this).parents('.check-form').hasClass('use-ajax')) {

			$.ajax({
         url: $(this).attr("action")+"?AJAX_REQUEST=Y",
         data: $(this).serialize() + "&web_form_submit=Y",
         type: 'post',

         success: function (data) {
            $(".popup-success").fadeIn(300).addClass('popup-opened');
						$(".popup-shadow").fadeIn(200);
						form.trigger('reset');
         },
         error: function (data) {
            $(".popup-error").fadeIn(300).addClass('popup-opened');
						$(".popup-shadow").fadeIn(200);
         }
      });
			return false;
		}

	});


//popup открытие, закрытие
$(".popup-open").click(function(e) {
	e = e || event;
	e.preventDefault();
	var popUp = $(this).attr("data-popup");
	if ($(this).hasClass('agreement_popup-open')){
		popUp = 'agreement';
	}

	$(".popup-block[data-popup='"+popUp+"']").fadeIn(300);

	if ($(".popup-opened").length) {
		$(".popup-block[data-popup='"+popUp+"']").addClass('second_popup-opened');
		$('.popup-opened').addClass('popup-block_shadow');
	} else{
		$(".popup-block[data-popup='"+popUp+"']").addClass('popup-opened');
		//$("body").toggleClass("body-lock");
		$(".popup-shadow").fadeIn(200);
	}
});

$(".popup-shadow, .popup__close, .popup-close").click(function() {

	//$(".popup-opened").find('iframe').stopVideo();

var video = $(".popup-opened").find('iframe').attr("src");
$(".popup-opened").find('iframe').attr("src","");
$(".popup-opened").find('iframe').attr("src",video);


	if ($(".second_popup-opened").length) {
		$(".second_popup-opened").fadeOut(300, function() {
			$(this).removeClass('second_popup-opened');
		});
		$('.popup-opened').removeClass('popup-block_shadow');
	} else{
		$("body").removeClass("body-lock");
		$(".popup-shadow").fadeOut(200);
		$(".popup-block").fadeOut(300).removeClass('popup-opened');
	}

	
});
    

	//Имя файла для input типа file
	$('input#upload').change(function() {
		var str = $(this).val();
		if (str.lastIndexOf('\\')){
			var i = str.lastIndexOf('\\')+1;
		}
		else{
			var i = str.lastIndexOf('/')+1;
		}	
		var filename = str.slice(i);
		$(this).siblings('#fileformlabel').html(filename);
	});

	$('.ad-line-close').on('click',function() {
		$('#ad-line').slideUp(300);
	});

	

});

$(document).mouseup(function (e) {
	var $target = $(e.target);
	if ($target.closest(".menu__tab").length == 0 && $target.closest(".has-sub.active").length == 0  && $target.closest('.megamenu').length == 0)  {
		$(".menu__tab.active").slideUp(300);
		$(".menu__tab.active").removeClass('active');
		$(".has-sub.active").removeClass('active');
		//$(".logo.active").removeClass('active');
	}
	if ($target.closest(".category-list").length == 0) {
		$(".category-list").removeClass('active');
	}
});
