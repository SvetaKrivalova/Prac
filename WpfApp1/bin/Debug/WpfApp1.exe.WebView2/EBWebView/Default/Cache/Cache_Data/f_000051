; /* Start:"a:4:{s:4:"full";s:59:"/local/templates/kemsu/js/special_version.js?15275628495052";s:6:"source";s:44:"/local/templates/kemsu/js/special_version.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(function () {

	$(document)
		.on('click', '.special-settings a', function (event) {
			event.preventDefault();
			$(this).siblings('a').removeClass('a-current');
			setSpecialVersion($(this).data());
		})
		.on('click', '[data-aa-off]', function (event) {
			unsetSpecialVersion();
			$(location).attr('href','?set-aa=normal');
		})
		.on('click', '[data-aa-on]', function (event) {
			event.preventDefault();
			setDefaultsSpecialVersion();

			if($(this).parent().hasClass('aa-hide')) {
				$(this).parent().html('<a href="/?set-aa=normal" title="Обычная версия" data-aa-off></a>');
			}			
		});

	// Запускаем функцию спецверсии
	jQuery(document).ready(function($) {
		setSpecialVersion();
		//console.log(Cookies.set('aaSet'));
	});

	/**
	 * Установка параметров отображения спецверсии
	 * @param {object} data Объект с данными для формирования параметров
	 */
	function setSpecialVersion(data) {
		var
			// получаем и сохраняем текущее значение нужной куки;
			$html = $('html'),
			// Получаем текущее значение атрибута class и html.
			htmlCurrentClass = $html.prop('class'),
			// Удаляем старые классы у html, оставляем только чужие классы, это важно, 
			// т.к. классы добавляем не только мы.
			clearSpecialClasses = htmlCurrentClass.replace(/special-([a-z,A-Z,-]+)/g, ''),
			// Признак включенной спецверсии, он же специальный класс, который добавится к html
			$aaVersion = {
				'aaVersion': 'on'
			},
			// Переменная под новые классы.
			htmlClass = '';

		// Если переданы данные
		if (data) {
			// Объединяем существующие куки с новыми данными из ссылки.
			var $newCookies = $.extend(Cookies.getJSON('aaSet'), data, $aaVersion);
			// Записываем новую куку
			Cookies.set('aaSet', $newCookies, {
				expires: 365,
				path: '/',
				secure: false
			});
		}

		// Удаляем ненужные классы a-current.
		//$('.a-current').removeClass('a-current');
		// Если есть кука — работаем.
		if (Cookies.getJSON('aaSet')) {
			// Пробегаем по массиву из нашей куки
			$.each(Cookies.getJSON('aaSet'), function (key, val) {
				// Формируем строку с классами, добавляемыми к html
				htmlClass += ' special-' + key + '-' + val;
				if (key) {
					var selector = ('.' + key + '-' + val + '');
					// Добавляем нужные классы a-current в панель.
					$(selector).addClass('a-current');
				};
			});

			$html
			// Заменяем текущий атрибут на очищенный от лишних классов.
				.prop('class', clearSpecialClasses)
				// Добавляем вновь сформированные классы.
				.addClass(htmlClass);
		}


		return false;
	}

	/**
	 * Отключение специальной версии сайта.
	 */
	function unsetSpecialVersion() {
		var
			// Получаем значение класса тега html.
			htmlCurrentClass = $('html').prop('class'),
			// Очищаем от классов спецверсии
			clearSpecialClasses = htmlCurrentClass.replace(/special-([a-z,A-Z,-]+)/g, '');
		// Заменяем текущий атрибут на очищенный от лишних классов.
		$('html').prop('class', clearSpecialClasses);

		$('.special-settings').find('.a-current').removeClass('a-current');
		$('.special-settings').find('.aaFontsize-normal').addClass('a-current');
		$('.special-settings').find('.aaImage-on').addClass('a-current');
		$('.special-settings').removeClass('active');
		// Удаляем куки
		Cookies.remove('aaSet', {
			path: '/'
		});

	}

	/**
	 * Установка дефолтных значений для спецверсии.
	 * @param {object} params Объект с данными для формирования параметров.
	 */
	function setDefaultsSpecialVersion(params) {
		// Задаём значения по умолчанию
		var $specialDefaults = {
			'aaVersion': 'on',
			'aaColor': 'black',
			'aaFontsize': 'small',
			'aaFont': 'tahoma',
			'aaKerning': 'normal',
			'aaImage': 'on'
		};

		// Объединяем значения по умолчанию с переданными данными.
		var $setDefaulParams = $.extend($specialDefaults, params);

		// Вызываем setSpecialVersion, где и происходит весь процесс.
		setSpecialVersion($setDefaulParams);
		$('.special-settings').addClass('active');
	}

})();
/* End */
;
; /* Start:"a:4:{s:4:"full";s:77:"/local/templates/kemsu/libs/scrollbar/jquery.scrollbar.min.js?151366455912248";s:6:"source";s:61:"/local/templates/kemsu/libs/scrollbar/jquery.scrollbar.min.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
